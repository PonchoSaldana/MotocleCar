#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/Alfonso/OneDrive/Escritorio/MotocleCar/MotocleCar/node_modules/.pnpm/ts-jest@29.4.3_@babel+core@_6c2fc696971d9e6cdd22616240276264/node_modules/ts-jest/node_modules:/mnt/c/Users/Alfonso/OneDrive/Escritorio/MotocleCar/MotocleCar/node_modules/.pnpm/ts-jest@29.4.3_@babel+core@_6c2fc696971d9e6cdd22616240276264/node_modules:/mnt/c/Users/Alfonso/OneDrive/Escritorio/MotocleCar/MotocleCar/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/Alfonso/OneDrive/Escritorio/MotocleCar/MotocleCar/node_modules/.pnpm/ts-jest@29.4.3_@babel+core@_6c2fc696971d9e6cdd22616240276264/node_modules/ts-jest/node_modules:/mnt/c/Users/Alfonso/OneDrive/Escritorio/MotocleCar/MotocleCar/node_modules/.pnpm/ts-jest@29.4.3_@babel+core@_6c2fc696971d9e6cdd22616240276264/node_modules:/mnt/c/Users/Alfonso/OneDrive/Escritorio/MotocleCar/MotocleCar/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../cli.js" "$@"
else
  exec node  "$basedir/../../cli.js" "$@"
fi
